{
  "name": "chess-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_PUBLIC_APP_ENV=staging next dev",
    "start": "cross-env NEXT_PUBLIC_APP_ENV=production next start",
    "build": "next build",
    "lint:code": "eslint src && prettier src --check",
    "lint:css": "stylelint --config .stylelintrc.json \"src/**/*.(css|scss)\"",
    "lint:next": "next lint",
    "lint": "yarn lint:code && yarn lint:css",
    "typecheck": "tsc --skipLibCheck --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.4.0",
    "@next/eslint-plugin-next": "^14.2.3",
    "@svgr/webpack": "^8.1.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/parser": "^7.12.0",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.2",
    "husky": "^9.0.11",
    "globals": "^15.12.0",
    "lint-staged": "^15.2.5",
    "postcss": "^8.4.38",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.1",
    "sass": "1.43.5",
    "stylelint": "^16.6.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.0.1"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": "18"
  }
}
